<%= error_messages_for 'static' %>

<h1>Editing "<%= @static.name %>"</h1>
<div class="form-box">
<%= form_tag({:action => 'static_update', :id => @static}, :multipart => true, :id => 'static_form') %>
  <%= render :partial => 'static_form' %>
  <%= submit_tag 'Edit' %>

<%= end_form_tag %>
</div>
<p>
<%= link_to 'Back', :action => 'static_list' %>
</p>