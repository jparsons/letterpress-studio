<%= error_messages_for 'artist' %>

<h1>Editing "<%= @artist.name %>"</h1>
<div class="form-box">
<%= form_tag({:action => 'artist_update', :id => @artist}, :multipart => true, :id => 'artist_form') %>
  <%= render :partial => 'artist_form' %>
  <%= submit_tag 'Edit' %>

<%= end_form_tag %>
</div>
<p>
<%= link_to 'Back', :action => 'artist_list' %>
</p>