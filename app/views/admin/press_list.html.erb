<h1>Presses</h1>


<h4><em>+ <%= link_to 'New Press', new_press_path %></em></h4>
  
<% for press in @presses %>
  
<h2><em><%= press.name %></em></h2>

<h3>
<%= link_to 'Edit', edit_press_path(press) %> / <%= link_to 'Destroy', press, :confirm => 'Are you sure?', :method => :delete %> </h3>

<% end %>
