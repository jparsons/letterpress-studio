<%= error_messages_for 'note' %>

<h1>Editing "<%= @note.name %>"</h1>
<div class="form-box">
<%= form_tag({:action => 'note_update', :id => @note}, :multipart => true, :id => 'note_form') %>
  <%= render :partial => 'note_form' %>
  <%= submit_tag 'Edit' %>

<%= end_form_tag %>
</div>
<p>
<%= link_to 'Back', :action => 'note_list' %>
</p>