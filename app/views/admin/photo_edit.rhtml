<%= error_messages_for 'photo' %>

<h1>Editing Image</h1>
<div class="form-box">
<%= form_tag({:action => 'photo_update', :id => @photo}, :multipart => true, :id => 'photo_form') %>
  <%= render :partial => 'photo_edit_form' %>
  <%= submit_tag 'Edit' %>

<%= end_form_tag %>
</div>
<p>
<%= link_to 'Back', :action => 'photo_list' %>
</p>