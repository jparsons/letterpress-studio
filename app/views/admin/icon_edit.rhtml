<%= error_messages_for 'icon' %>

<h1>Editing Image</h1>
<div class="form-box">
<%= form_tag({:action => 'icon_update', :id => @icon}, :multipart => true, :id => 'icon_form') %>
  <%= render :partial => 'icon_edit_form' %>
  <%= submit_tag 'Edit' %>

<%= end_form_tag %>
</div>
<p>
<%= link_to 'Back', :action => 'icon_list' %>
</p>